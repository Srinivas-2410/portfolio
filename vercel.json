{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server.js"
    }
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "includeFiles": ["dist/**", "public/**"]
      }
    }
  ]
}
